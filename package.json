{
  "name": "vite_bundler",
  "private": true,
  "homepage": "https://vladtoby.github.io/practicum_react_project/index.html",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open --config-file cypress.config.cjs",
    "cypress:run": "cypress run --config-file cypress.config.cjs",
    "test:e2e": "start-server-and-test dev http://localhost:5173 cypress:run",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.4",
    "@redux-devtools/extension": "^3.3.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@ya.praktikum/react-developer-burger-ui-components": "1.14.2",
    "axios": "^1.7.9",
    "core-js": "^3.38.1",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^14.3.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.3.0",
    "happy-dom": "^13.3.8",
    "jsdom": "^24.0.0",
    "sass": "^1.83.4",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  }
}
